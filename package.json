{
  "name": "simple-fxtrade",
  "version": "1.0.1",
  "description": "A node js wrapper for the Oanda Rest v20 api to make things really simple",
  "main": "dist/node.js",
  "browser": "dist/browser.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lteacher/simple-fxtrade.git"
  },
  "engines": {
    "node": ">=7.7.1"
  },
  "scripts": {
    "build": "webpack -p",
    "clean": "rm -r dist/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc --reporter=lcov --reporter=text mocha -t 10000 --compilers js:babel-core/register --recursive --bail test/unit test/integration",
    "test-watch": "mocha -t 10000 --compilers js:babel-core/register test/unit test/integration -w --recursive --bail"
  },
  "author": "hugo.armstrong@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "dotenv": "^4.0.0",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "testdouble": "^2.1.2",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3"
  },
  "keywords": [
    "forex",
    "oanda",
    "fxtrade"
  ]
}
